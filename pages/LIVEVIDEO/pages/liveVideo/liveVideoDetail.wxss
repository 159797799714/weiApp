/* pages/LIVEVIDEO//pages/liveVideo/liveVideoDetail.wxss */

page, .container {
  margin: 0;
  padding: 0;
}

.live-player {
  width: 100%;
  height: 100vh;
  vertical-align: middle;
  position: absolute;
  left: 0;
  top: 0;
  overflow: hidden;
}

/* 直播页信息 */

.live-playered {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
  width: 100%;
  min-height: 100%;
  background-color: rgba(0, 0, 0, 0);
}

.img-cover {
  width: 104%;
  height: 104%;
  vertical-align: middle;
  filter: blur(3px);
  margin: -2% 0 0 -2%;
}

/* 直播者信息 */

.user-mes {
  position: absolute;
  left: 110rpx;
  top: 52rpx;
  z-index: 1;
  background-color: rgba(0, 0, 0, 0.38);
  color: #fff;
  font-size: 26rpx;
  margin-top: -3rpx;
  padding: 3rpx 14rpx 3rpx 3rpx;
  border-radius: 66rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  overflow: hidden;
}

.user-avatar {
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 8rpx;
}

.user-content {
  margin-right: 12rpx;
}

.user-content cover-view:first-child {
  max-width: 120rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.look-num {
  font-size: 24rpx;
  margin-top: 4rpx;
}

.look-nums {
  margin-right: 10rpx;
}

.look-num cover-view {

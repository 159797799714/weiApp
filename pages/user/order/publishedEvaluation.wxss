/* pages/user/order/returnGoods.wxss */
.orderShop .clearfix .imgList{
  position: relative;
}
.orderShop .clearfix .imgList .iconbox{
  width: auto;
  height: auto;
  display: flex;
  justify-content: center;
  align-content: center;
  margin: 0;
  border: none;
  position: absolute;
  top: 0;
  right: 0;
  border-radius: 0 0 0 8rpx;
  overflow: hidden;
}
.orderShop .clearfix .imgList .iconbox .iconmask{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  margin: 0;
  border: none;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.orderShop .clearfix .imgList .iconbox .iconfont{
  font-size: 26rpx;
  color: #fff;
  font-weight: bold;
  line-height: 26rpx;
  padding: 15rpx;
  position: relative;
  z-index: 2;
}
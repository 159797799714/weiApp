/* pages/SHOPKEEPER//pages/shop/index.wxss */
page{
  background: #F0F0F0;
}
.shoper-page .main {
  width: 100vw;
  /* margin-top: 20rpx; */
}

.shoper-page .main .msg-lists {
  width: 100%;
}

/* 模块一 */

.shoper-page .main .msg-lists .msg-list.style1 {
  width: 100%;
  padding: 32rpx 40rpx;
  background: #fff;
  box-sizing: border-box;
}
.msg-list-box{
  padding: 24rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.msg-list.style1{
  margin-top: 20rpx;
}

/* 图片板块 */

.img-block {
  width: 100%;
}
.style2 .img-block{
  margin-top: 24rpx;
  border-radius: 16rpx 16rpx 0 0;
  overflow: hidden;
}
.img-block.pic-num-2 {
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}

.img-block.pic-num-1 image {
  display: block;
  max-width: 100%;
  max-height: 100vh;
  border-radius: 8rpx;
  /* margin-bottom: 20rpx; */
}

.img-block.pic-num-2 image {
  display: block;
  width: 324rpx;
  height: 324rpx;
  border-radius: 8rpx;
  /* margin-bottom: 20rpx; */
}

.img-block.pic-num-3, .img-block.pic-num-5, .img-block.pic-num-6,
.img-block.pic-num-7, .img-block.pic-num-8, .img-block.pic-num-9 {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.img-block.pic-num-3 image, .img-block.pic-num-5 image,
.img-block.pic-num-6 image, .img-block.pic-num-7 image,
.img-block.pic-num-8 image, .img-block.pic-num-9 image {
  width: 210rpx;
  height: 210rpx;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}

.img-block.pic-num-5:after, .img-block.pic-num-8:after {
  content: '';
  width: 210rpx;
}

.img-block.pic-num-4 {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.img-block.pic-num-4 image {
  width: 324rpx;
  height: 324rpx;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}

.soft-content {
  width: 100%;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #333;
  line-height: 36rpx;
  padding: 30rpx 20rpx 0;
  box-sizing: border-box;
}
.soft-content.hide-more{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 6;
  overflow: hidden;
}
.slide-block{
  font-size: 32rpx;
  margin-top: 8rpx;
}
.oprate {
  width: 100%;
  /* height: 40rpx; */
  /* display: flex;
  justify-content: space-between;
  align-items: center; */
  overflow: hidden;
  color: #999;
  font-size: 30rpx;
  /* margin-top: 20rpx; */
  padding: 20rpx 20rpx 30rpx;
  box-sizing: border-box;
}
.oprate .left,.oprate .right{
  display: flex;
}

.end {
  color: #919398;
  font-size: 24rpx;
  font-weight: 600;
  text-align: center;
}

.end text {
  margin: 0 10rpx;
}

.end::before, .end::after {
  content: '';
  display: inline-block;
  width: 100rpx;
  height: 1px;
  background-color: #919398;
  vertical-align: middle;
}
.icon-loading{
  display: inline-block;
  vertical-align: middle;
  font-size: 36rpx;
  animation: myroate 1s linear infinite;
}
@keyframes myroate{
  from{transform: rotate(0deg);}
  to{transform: rotate(360deg);}
}
/* 模板二 */

.msg-lists.type2 {
  width: 100%;
  /* box-sizing: border-box; */
  /* padding: 24rpx; */
  overflow: hidden;
  /* column-count: 2;
  column-gap: 22rpx; */
  /* margin: 2em auto; */
}
.item-left{
  float: left;
  width: 340rpx;
}
.item-right{
  float: right;
  width: 340rpx;
}
.items{
  margin-bottom: 20rpx;
}
.item-left>view:first-child .img-block,.item-right>view:first-child .img-block{
  margin-top: 0;
}
.shoper-page .main .msg-lists .msg-list.style2 {
  /* width: 340rpx; */
  break-inside: avoid;
  margin-bottom: 24rpx;
  width: 100%;
}
.shoper-page .main .msg-lists .msg-list.style2:nth-child(odd){
  margin-right: 22rpx;
}

.shoper-page .main .msg-lists .msg-list.style2 .img-block image {
  width: 100%;
  max-height: 100vh;
}

.soft-content.style2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  background: #fff;
}

.oprate.style1 {
  display: flex;
}
.oprate.style2 {
  font-size: 24rpx;
}

.oprate.style2 .read {
  color: rgba(192, 196, 204, 1);
}
.read.cricle{
  position: relative;
}
.read.cricle::before{
  position: absolute; 
  content: '';
  left: -24rpx;
  top: 16rpx;
  display: block;
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background: rgba(225,226,230,1);
  
}

.oprate.style2 .like {
  color: #999;
  font-size: 30rpx;
  line-height: 40rpx;
}
.oprate.style2 .like text{
  color: #666;
  font-size: 30rpx;
  margin-right: 15rpx;
  margin-top: 4rpx;
}
.go-like{
  display: flex;
  align-items: center;
  float: right;
}
.go-like text{
  margin-right: 6rpx;
}
.like{
  float: right;
}
.like .user-icon{
  color:rgba(145,147,152,1);
  background:transparent;
  display: flex;
  align-items: center;
  padding-right: 0;
  line-height: initial;
}
.like .user-icon::after{
  border: none;
}
.type1 .iconfont{
  font-size: 40rpx;
}
.type1 .like .user-icon .like-num{
  font-size: 30rpx;
  margin-left: 6rpx;
}
.type2 .like .user-icon .like-num{
  font-size: 24rpx;
  margin-left: 6rpx;
}
.list-contener {
  background: #fff;
  box-sizing: border-box;
  border-radius: 0 0 16rpx 16rpx;
}
/* .style2 .list-contener{
  padding: 20rpx;
} */
/* 空数据 */

.data_null_block {
  width: 100%;
  min-height: 420rpx;
  margin: 344rpx auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.data_null_block .tips {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(192, 196, 204, 1);
  line-height: 42rpx;
  margin-top: -50rpx;
}
.style2 .img-block image{
  margin-bottom: 0;
  vertical-align: middle;
  border-radius: 16rpx 16rpx 0 0;
}
/* 视频 */
.videoed{
  margin-bottom: 0;
}
.vedio-block{
  width: 100%;
  border-radius: 16rpx 16rpx 0 0;
  overflow: hidden;
}
.type1 .vedio-block{
  margin-bottom: 20rpx;
}
.video_label{
  width: 100%;
  vertical-align: middle;
  border-radius: 16rpx 16rpx 0 0;
}
.oprate .left {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  float: left;
}
.style1 .left{
  flex: 1;
}
.read {
  max-width: 50%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
  height: 40rpx;
  line-height: 42rpx;
}
.dot {
  margin-right: 24rpx;
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background-color: #E1E2E6;
}
.publisher{
  float: left;
  margin-right: 10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 40rpx;
}
.publisher image{
  width: 15px;
  height: 15px;
  border-radius: 50%;
  vertical-align: sub;
  margin-right: 10rpx;
}
.publisher text{
  max-width: 180rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: sub;
  display: inline-block;
}
/* 搜索框 */
.search-cloth{
  height: 110rpx;
}
.topicSearch{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 13px;
  /* border-bottom: 1px solid #eaeaea; */
  padding: 24rpx;
  width: calc(100% - 48rpx);
  background-color: #fff;
  position:fixed;
  left:0;
  z-index: 1000;
}
.search-ipt{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border: 1px solid #F0F0F0;
  background-color: #F0F0F0;
  border-radius: 30rpx;
  overflow: hidden;
  width: calc(100% - 138rpx);
  padding: 2px 14rpx;
}
.search-ipt input{
  width: calc(100% - 50rpx);
  margin-left: 10rpx;
}
.search-ipt .input-place{
  color: #999;
  font-size: 26rpx;
}
.go-back{
  width: 90rpx;
  color: #333;
  font-size: 28rpx;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  background: none;
}
.oprate.style2 .like .icon-xihuan, .oprate .like .icon-xihuan{
  color: #EB4257;
}
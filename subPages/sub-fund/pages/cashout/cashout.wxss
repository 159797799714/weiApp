page{width:100vw;height:100vh;padding:0;background:#f2f3f5}.cashout-wrap{padding:30rpx}.cashout-wrap .balance-part{background:linear-gradient(90deg, #4A4844 0%, #282825 100%);padding:30rpx 40rpx;margin-bottom:40rpx;border-radius:8rpx;position:relative}.cashout-wrap .balance-part .actitle{font-size:28rpx;color:#fff;line-height:40rpx;margin-bottom:20rpx}.cashout-wrap .balance-part .acval{max-width:410rpx;font-size:68rpx;color:#F6E7C7;line-height:60rpx}.cashout-wrap .balance-part .tocas{width:180rpx;height:60rpx;font-size:28rpx;color:#F6E7C7;display:flex;align-items:center;justify-content:center;background:#59544c;border-radius:60rpx;position:absolute;top:50%;right:40rpx;transform:translateY(-50%)}.cashout-wrap .title{font-size:28rpx;color:#333;line-height:40rpx;margin-bottom:16rpx}.cashout-wrap .blocks{background:#fff;margin-bottom:20rpx;padding:34rpx 30rpx 30rpx;border-radius:8rpx}.cashout-wrap .input-box{display:flex;align-items:center;padding:25rpx 0;margin-bottom:20rpx}.cashout-wrap .input-box .unit{font-size:58rpx;color:#333;line-height:68rpx;margin-right:10rpx}.cashout-wrap .input-box .input-and-place{flex:1;position:relative}.cashout-wrap .input-box .cainput{width:100%;height:68rpx;font-size:58rpx;color:#333;line-height:68rpx;font-weight:500;position:relative;z-index:2}.cashout-wrap .input-box .place{width:100%;height:68rpx;font-size:26rpx;color:#999;line-height:68rpx;position:absolute;top:0;left:0;z-index:1}.cashout-wrap .input-box .all{font-size:26rpx;color:#FF4444;line-height:68rpx;margin-left:10rpx}.cashout-wrap .min{font-size:26rpx;color:#999;line-height:37rpx}.cashout-wrap .account-part{margin-bottom:30rpx}.cashout-wrap .infos{display:flex;align-items:center;font-size:28rpx;color:#666;line-height:40rpx}.cashout-wrap .infos .iconfont{font-size:38rpx;color:#40BA49;line-height:38rpx;margin-right:20rpx}.cashout-wrap .savebtn{display:flex;align-items:center;justify-content:center;font-size:34rpx;color:#fff;background:#D1A96F;margin-bottom:50rpx;border-radius:8rpx}.cashout-wrap .savebtn::after{display:none}.cashout-wrap .savebtn.unAllowed{background:#c4c4c4}.cashout-wrap .tips .tptitle{width:336rpx;margin:0 auto 40rpx;border-bottom:1rpx solid #DDDDDD}.cashout-wrap .tips .tptitle .tcnt{width:168rpx;font-size:30rpx;color:#333;line-height:42rpx;text-align:center;background:#f2f3f5;margin:0 auto -24rpx}.cashout-wrap .tips .tpitem{display:flex;font-size:26rpx;color:#747474;line-height:37rpx;margin-bottom:24rpx}.cashout-wrap .tips .tpitem .order{margin-right:10rpx}.cashout-modal-body{width:540rpx;background:#fff;padding-bottom:60rpx}.cashout-modal-body .top-icons{width:540rpx;height:240rpx;display:flex;align-items:center;justify-content:center;background:url('https://frontend-mam.oss-cn-hangzhou.aliyuncs.com/live/cashout_bg.png') no-repeat;background-size:contain;margin-bottom:50rpx}.cashout-modal-body .top-icons .iconfont{font-size:90rpx;color:#fff;text-align:center;line-height:90rpx}.cashout-modal-body .title{font-size:32rpx;color:#333;line-height:45rpx;margin-bottom:20rpx}.cashout-modal-body .cnts{width:450rpx;font-size:26rpx;color:#5f5f5f;line-height:37rpx;text-align:left;margin:0 auto}.cashout-modal-body .cnts .rd{color:#f64445}.cashout-modal-body .cnts .gry-last{color:#999}.cashout-modal-footer{background:#fff;padding-bottom:50rpx}.casbtn{width:420rpx;height:80rpx;display:flex;justify-content:center;align-items:center;font-size:32rpx;color:#fff;line-height:45rpx;margin:0 auto;background:linear-gradient(90deg, #FF7979 4%, #FF4444 100%);border-radius:100rpx}.casbtn::after{display:none}